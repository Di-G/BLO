{
	"version": 2,
	"framework": null,
	"cleanUrls": true,
	"trailingSlash": false,
	"headers": [
		{
			"source": "/sw.js",
			"headers": [
				{ "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" }
			]
		},
		{
			"source": "/manifest.webmanifest",
			"headers": [
				{ "key": "Cache-Control", "value": "no-cache" }
			]
		},
		{
			"source": "/script.js",
			"headers": [
				{ "key": "Cache-Control", "value": "no-cache" }
			]
		}
	],
	"rewrites": [
		{ "source": "/(.*)", "destination": "/index.html" }
	]
}

